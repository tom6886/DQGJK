<link href="~/js/select2/select2.css" rel="stylesheet" />
<div class="row">
    <div class="col-md-12">
        <div class="content-panel panel panel-default">
            <div class="panel-body">
                <div class="row" style="margin-bottom: 15px;">
                    <div class="col-md-1"></div>
                    <div class="col-md-2">
                        <div class="input-group">
                            <span class="input-group-addon">省</span>
                            <input type="hidden" data-select="p" class="form-control _select" name="Province" />
                        </div>
                    </div>
                    <div class="col-md-2">
                        <div class="input-group">
                            <span class="input-group-addon">市</span>
                            <input type="hidden" data-select="ci" class="form-control _select" name="City" />
                        </div>
                    </div>
                    <div class="col-md-2">
                        <div class="input-group">
                            <span class="input-group-addon">县</span>
                            <input type="hidden" data-select="co" class="form-control _select" name="Country" />
                        </div>
                    </div>
                    <div class="col-md-2">
                        <div class="input-group">
                            <span class="input-group-addon">环网柜</span>
                            <input type="text" data-select="s" class="form-control _select" name="stationID" />
                        </div>
                    </div>
                    <div class="col-md-3">
                        <a class="btn btn-info query" href="javascript:void(0)">查询</a>
                    </div>
                </div>
                <section id="unseen">
                    <div class="panel panel-default">
                        <div class="panel-body">
                            <span>长江路1#环网柜</span>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-6">
                            <div class="panel panel-info">
                                <div class="panel-body">
                                    <div id="chart0" class="_line" data-title="#1柜月平均温湿度" style="min-width:400px;height:250px"></div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="panel panel-info">
                                <div class="panel-body">
                                    <div id="chart1" class="_line" data-title="#2柜月平均温湿度" style="min-width:400px;height:250px"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-6">
                            <div class="panel panel-info">
                                <div class="panel-body">
                                    <div id="chart2" class="_line" data-title="#3柜月平均温湿度" style="min-width:400px;height:250px"></div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="panel panel-info">
                                <div class="panel-body">
                                    <div id="chart3" class="_line" data-title="#4柜月平均温湿度" style="min-width:400px;height:250px"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>
            </div>
        </div>
    </div>
</div>

<script type="text/javascript" src="~/js/select2/select2.min.js"></script>
<script type="text/javascript" src="~/js/select.js"></script>

<script type="text/javascript" src="~/js/highcharts/highcharts.js"></script>
<script type="text/javascript" src="~/js/highcharts/highcharts-more.js"></script>

<script type="text/javascript" src="~/js/linechart/linechart.js"></script>

<script type="text/javascript">
    $(function () {
        $("._select").select_2();

        $("._line").linechart();
    });
</script>

<script type="text/javascript" src="~/js/Page/trend.js"></script>
